<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Hub Spot Test Automation Regression Suite"
	thread-count="3" parallel="tests" verbose="3">

	<listeners>

		<listener
			class-name="com.qa.hubspot.listeners.ExtentReportListener" />
		<!-- <listener class-name="com.qa.hubspot.listeners.pdfListener" /> <listener 
			class-name="com.qa.hubspot.listeners.TestAllureListener" /> -->

	</listeners>


	<test name="Login Page Test">
		<classes>
			<class name="com.qa.hubspot.tests.LoginPageTest" />
		</classes>
	</test>

	<test name="Home Page Test">
		<classes>
			<class name="com.qa.hubspot.tests.HomePageTest" />
		</classes>
	</test>
	<test name="Contacts Page Test">
		<classes>
			<class name="com.qa.hubspot.tests.ContactsPageTest" />
		</classes>
	</test>

</suite> 
